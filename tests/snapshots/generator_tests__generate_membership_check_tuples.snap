-- teams membership from team_members
SELECT 'teams:' || "team_id" AS object, 'member' AS relation, 'user:' || "user_id" AS subject
FROM "team_members"
WHERE "team_id" IS NOT NULL AND "user_id" IS NOT NULL;

-- projects to teams bridge for tuple-to-userset
SELECT 'projects:' || "id" AS object, 'teams' AS relation, 'teams:' || "team_id" AS subject
FROM "projects"
WHERE "id" IS NOT NULL
AND "team_id" IS NOT NULL;
